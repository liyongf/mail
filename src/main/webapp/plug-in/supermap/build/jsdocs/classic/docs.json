{
    "opts": {
        "recurse": true,
        "template": "../template",
        "destination": "./docs/classic"
    },
    "disqus":"",
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js?$",
        "excludePattern": ".+\\\\src\\\\common\\\\iServer\\\\.*Service|Base|Parameters.js?$",
        "include": [
            "src/classic","src/common/thirdparty"
        ],
        "exclude": [
            "src/common/overlay/feature","src/common/overlay/levelRenderer"
        ]
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
        //"applicationName": "iClient Classic"
    }
}
